<h1>Edit Location</h1>
<%= render '/shared/error_messages', object: @location %>
<%= form_for @location, class: "form col-lg-4" do |form| %>
  <%= form.text_field :name, class: "form-control", placeholder: "Name" %>
  <div class="row">
    <div class="span3 lead">
      <%= form.submit "Update Location" %>
    </div>
  </div>
<% end %>
